package labs;
import java.util.ArrayList;
import java.util.Scanner;
import java.io.*;
public class Wifi {
	private int [] myArray = new int [input.nextInt()];
	Wifi(){
	}
	
  
   public static getValue(int z){
     return myArray[z] ;
   }

 public static void main(String[] args)  {
	 Scanner input = new Scanner(new File("array1.txt"));
	 Wifi()

	 
	while (input.hasNext()){
	 int guests = input.nextInt();
	 System.out.println(11/guests);
	}
 }
}



