package labs;
import java.util.ArrayList;
import java.util.Scanner;
import java.io.*;
public class Wifi {
	private int [] myArray;
	Wifi(){
	}
	
	public void setMyArray(int [] myArray) {
		this.myArray = myArray;
	}
   public  int getValue(int z){
     return myArray[z] ;
   }

   public static void main(String[] args)  {
	try { Scanner input = new Scanner(new File("array1.txt"));
	}catch(Exception E ) 
	Wifi H = new Wifi();

	 
	 
	 
	while (input.hasNext()){
	 int guests = input.nextInt();
	 System.out.println(11/guests);
	}
input.close();
   }


 
}



